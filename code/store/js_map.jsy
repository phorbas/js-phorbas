
export ::
  js_map_store as default,
  js_map_store as map_store,

export async function js_map_store() ::
  const _db = new Map()

  return @{}
    uses_binary_key: false

    async store_at(key, enc_content) ::
      _db.set @ key, enc_content
      return true

    async fetch_at(key) ::
      return _db.get @ key

