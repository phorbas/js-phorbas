#IF PLAT_NODEJS
  import {createHash} from 'crypto'

  export const sha_256__node = async data => new Uint8Array @
    createHash('sha256').update(await data).digest()
  export const sha_256 = sha_256__node

#ELIF PLAT_WEB
  export const sha_256__web = crypto.subtle.digest
    .bind @ crypto.subtle, { name: "SHA-256" }
  export const sha_256 = sha_256__web

export default sha_256
